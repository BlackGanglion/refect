{
  "presets": ["react", "es2015", "stage-0"],
  "plugins": [
    ["react-hot-loader/babel"],
    ["import", { libraryName: "antd", style: "css" }],
    ["transform-decorators-legacy"],
    ["module-alias", [
      { "src": "./src/utils", "expose": "utils" },
      { "src": "./src/effects", "expose": "effects" },
    ]]
  ]
}
